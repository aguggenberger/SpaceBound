package com.aguggenberger.spacebound.screens;

import com.aguggenberger.spacebound.SpaceBound;
import com.aguggenberger.spacebound.actor.PlayerActor;

public class StartGameScreen extends AbstractScreen{
	
	private PlayerActor player;
	
	public StartGameScreen(SpaceBound game){		
	    super(game);
	    
	    player = new PlayerActor();
	    
	    stage.addActor(player);
	}
}